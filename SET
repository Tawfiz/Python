my_set = {1,2,2,3,4,4,4,4}
print("Set:", my_set)
my_set.add(5)
print("updated set:", my_set)
set1 = my_set
set2 = {1, 4, 4, 6}

print("set1:", set1)
print("set2:", set2)
print("Difference")
print(set1.difference(set2))
print("Symmeteric Difference")
print(set1.symmetric_difference(set2))
print("Union")
print(set1.union(set2))
print("Intersection")
print(set1.intersection(set2))
